<h1>Favorites</h1>

<div class="card-small-container">
  <% @parks.each do |park| %>
    <%= render 'parks/park_thumb', park: park %>
  <% end %>
</div>

<h1>My Reviews</h1>
  <% if @reviews.any? %>
    <% @reviews.each do |review| %>
      <p><%= review.rating %></p>
      <p><%= review.content %></p>
    <% end %>
  <% else %>
    <p>No reviews yet!</p>
  <% end %>

</h1>

<% if @user == current_user %>
  <p><%= link_to "Edit profile", edit_user_path %></p>
<% end %>

